<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="env.Entities.FilledForm" table="filled_form">
      <meta attribute="class-description">
         This class contains the filled form detail. 
      </meta>
      <id name="filled_form_id" type="int" column="field_form_id">
         <generator class="native"/>
      </id>
	<many-to-one unique="true" cascade="update" class="env.Entities.Form">
		<column name="form_id" not-null="true"/>
	</many-to-one>
	<set name="field_id" cascade="update">
		<key column="field_id" not-null="true" />
		<one-to-many class="env.Entities.FormFields" />
	</set>
	<property name="value" column="value" type="string"/>
	<many-to-one unique="true" class="env.Entities.User">
		<column name="user_id" not-null="true"/>
	</many-to-one>
	<property name="record_id" column="record_id" type="int"/>
	<property name="isActive" column="isActive" type="boolean"/>
	<property name="timestamp" column="timestamp" type="java.sql.Timestamp"/>

   </class>
</hibernate-mapping>