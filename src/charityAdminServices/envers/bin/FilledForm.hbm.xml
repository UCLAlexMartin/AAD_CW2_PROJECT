<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="env.Entities.FilledForm" table="filled_form">
      <meta attribute="class-description">
         This class contains the filled form detail. 
      </meta>
      <id name="form_id" type="int" column="form_id">
         <generator class="native"/>
      </id>
      <property name="column_id" column="column_id" type="int"/>
      <property name="value" column="value" type="string"/>
      <property name="user_id" column="user_id" type="int"/>
      <property name="isActive" column="isActive" type="boolean"/>
   </class>
</hibernate-mapping>