<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 <hibernate-mapping>
  <class name="env.Entities.FormPermissions" table="form_permissions">
      <meta attribute="class-description">
         This class contains the filled form detail. 
      </meta>
      <id name="form_permission_id" type="int" column="form_permission_id">
         <generator class="native"/>
      </id>
      <one-to-many name="form" class="env.Entities.Form" cascade="update">
      		 <column name="form_id" not-null="true" />
      </one-to-many>
      <one-to-many name="user_type" class="env.Entities.UserType" type="string">
      		<column name="user_Type_Id" not-null="true"/>
      </one-to-many>
      <property name="permission" column="Permission" type="string"/>
      <property name="isActive" column="isActive" type="boolean"/>
      <property name="timestamp" column="TimeStamp" type="java.sql.Timestamp"/>
   </class>
 </hibernate-mapping>
 